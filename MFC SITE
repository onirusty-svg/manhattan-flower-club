  <script>
    const defaultConfig = {
      background_color: "#0a0a0a",
      gold_color: "#d4af37",
      text_color: "#c9c9c9",
      button_bg_color: "#d4af37",
      button_text_color: "#0a0a0a",
      font_family: "Playfair Display",
      font_size: 72,
      hero_headline: "Where Luxury Blooms",
      hero_subheadline: "Curated floral artistry for those who hold the key.",
      hero_primary_btn: "Enter the Club",
      hero_secondary_btn: "Discover the Collection",
      brand_title: "The Manhattan Flower Club",
      brand_text: "A floral house defined by craftsmanship, exclusivity, and timeless elegance. Each arrangement is designed as an object of beauty and distinction.",
      brand_btn: "Learn More",
      collection_title: "Featured Collection",
      collection_btn: "View the Full Collection",
      membership_title: "A Private Floral Experience",
      membership_text: "Members receive access to exclusive designs, seasonal releases, and bespoke arrangements crafted beyond the public collection.",
      membership_btn: "Request Access",
      concierge_title: "Private Inquiries",
      concierge_text: "Bespoke arrangements, private events, and corporate gifting handled with discretion and care.",
      concierge_btn: "Submit Inquiry",
      journal_title: "Journal",
      journal_text: "Insights, inspiration, and craftsmanship from the world of Manhattan Flower Club.",
      journal_btn: "Read the Journal"
    };

    async function onConfigChange(config) {
      const backgroundColor = config.background_color || defaultConfig.background_color;
      const goldColor = config.gold_color || defaultConfig.gold_color;
      const textColor = config.text_color || defaultConfig.text_color;
      const buttonBgColor = config.button_bg_color || defaultConfig.button_bg_color;
      const buttonTextColor = config.button_text_color || defaultConfig.button_text_color;
      const fontFamily = config.font_family || defaultConfig.font_family;
      const fontSize = config.font_size || defaultConfig.font_size;

      // Update backgrounds
      document.querySelectorAll('.hero-section, .brand-section, .collection-section, .membership-section, .concierge-section, .journal-section, .footer').forEach(section => {
        section.style.background = backgroundColor;
      });

      // Update hero
      document.getElementById('hero-headline').textContent = config.hero_headline || defaultConfig.hero_headline;
      document.getElementById('hero-headline').style.color = goldColor;
      document.getElementById('hero-headline').style.fontFamily = `${fontFamily}, serif`;
      document.getElementById('hero-headline').style.fontSize = `${fontSize}px`;
      
      document.getElementById('hero-subheadline').textContent = config.hero_subheadline || defaultConfig.hero_subheadline;
      document.getElementById('hero-subheadline').style.color = textColor;
      
      document.getElementById('hero-primary-btn').textContent = config.hero_primary_btn || defaultConfig.hero_primary_btn;
      document.getElementById('hero-primary-btn').style.background = buttonBgColor;
      document.getElementById('hero-primary-btn').style.color = buttonTextColor;
      
      document.getElementById('hero-secondary-btn').textContent = config.hero_secondary_btn || defaultConfig.hero_secondary_btn;
      document.getElementById('hero-secondary-btn').style.color = goldColor;
      document.getElementById('hero-secondary-btn').style.borderColor = goldColor;

      // Update brand section
      document.getElementById('brand-title').textContent = config.brand_title || defaultConfig.brand_title;
      document.getElementById('brand-title').style.color = goldColor;
      document.getElementById('brand-title').style.fontFamily = `${fontFamily}, serif`;
      document.getElementById('brand-title').style.fontSize = `${fontSize * 0.78}px`;
      
      document.getElementById('brand-text').textContent = config.brand_text || defaultConfig.brand_text;
      document.getElementById('brand-text').style.color = textColor;
      
      document.getElementById('brand-btn').textContent = config.brand_btn || defaultConfig.brand_btn;
      document.getElementById('brand-btn').style.color = goldColor;
      document.getElementById('brand-btn').style.borderColor = goldColor;

      // Update collection section
      document.getElementById('collection-title').textContent = config.collection_title || defaultConfig.collection_title;
      document.getElementById('collection-title').style.color = goldColor;
      document.getElementById('collection-title').style.fontFamily = `${fontFamily}, serif`;
      document.getElementById('collection-title').style.fontSize = `${fontSize * 0.78}px`;
      
      document.getElementById('collection-btn').textContent = config.collection_btn || defaultConfig.collection_btn;
      document.getElementById('collection-btn').style.background = buttonBgColor;
      document.getElementById('collection-btn').style.color = buttonTextColor;

      // Update membership section
      document.getElementById('membership-title').textContent = config.membership_title || defaultConfig.membership_title;
      document.getElementById('membership-title').style.color = goldColor;
      document.getElementById('membership-title').style.fontFamily = `${fontFamily}, serif`;
      document.getElementById('membership-title').style.fontSize = `${fontSize * 0.78}px`;
      
      document.getElementById('membership-text').textContent = config.membership_text || defaultConfig.membership_text;
      document.getElementById('membership-text').style.color = textColor;
      
      document.getElementById('membership-btn').textContent = config.membership_btn || defaultConfig.membership_btn;
      document.getElementById('membership-btn').style.background = buttonBgColor;
      document.getElementById('membership-btn').style.color = buttonTextColor;

      // Update concierge section
      document.getElementById('concierge-title').textContent = config.concierge_title || defaultConfig.concierge_title;
      document.getElementById('concierge-title').style.color = goldColor;
      document.getElementById('concierge-title').style.fontFamily = `${fontFamily}, serif`;
      document.getElementById('concierge-title').style.fontSize = `${fontSize * 0.78}px`;
      
      document.getElementById('concierge-text').textContent = config.concierge_text || defaultConfig.concierge_text;
      document.getElementById('concierge-text').style.color = textColor;
      
      document.getElementById('concierge-btn').textContent = config.concierge_btn || defaultConfig.concierge_btn;
      document.getElementById('concierge-btn').style.color = goldColor;
      document.getElementById('concierge-btn').style.borderColor = goldColor;

      // Update journal section
      document.getElementById('journal-title').textContent = config.journal_title || defaultConfig.journal_title;
      document.getElementById('journal-title').style.color = goldColor;
      document.getElementById('journal-title').style.fontFamily = `${fontFamily}, serif`;
      document.getElementById('journal-title').style.fontSize = `${fontSize * 0.78}px`;
      
      document.getElementById('journal-text').textContent = config.journal_text || defaultConfig.journal_text;
      document.getElementById('journal-text').style.color = textColor;
      
      document.getElementById('journal-btn').textContent = config.journal_btn || defaultConfig.journal_btn;
      document.getElementById('journal-btn').style.color = goldColor;
      document.getElementById('journal-btn').style.borderColor = goldColor;

      // Update all SVG gold elements
      document.querySelectorAll('svg path[stroke="#d4af37"], svg circle[stroke="#d4af37"], svg line[stroke="#d4af37"], svg rect[fill="#d4af37"], svg ellipse[stroke="#d4af37"]').forEach(el => {
        if (el.hasAttribute('stroke')) el.setAttribute('stroke', goldColor);
        if (el.hasAttribute('fill') && el.getAttribute('fill') !== 'none') el.setAttribute('fill', goldColor);
      });

      // Update product names
      document.querySelectorAll('.product-name').forEach(name => {
        name.style.color = goldColor;
        name.style.fontFamily = `${fontFamily}, serif`;
      });

      // Update section titles and text
      document.querySelectorAll('.section-title').forEach(title => {
        title.style.color = goldColor;
        title.style.fontFamily = `${fontFamily}, serif`;
      });
      
      document.querySelectorAll('.section-text').forEach(text => {
        text.style.color = textColor;
      });

      // Update footer
      document.querySelectorAll('.footer-link').forEach(link => {
        link.style.color = goldColor;
      });
      
      document.querySelector('.footer-brand').style.color = goldColor;
    }

    function mapToCapabilities(config) {
      return {
        recolorables: [
          {
            get: () => config.background_color || defaultConfig.background_color,
            set: (value) => {
              config.background_color = value;
              if (window.elementSdk) {
                window.elementSdk.setConfig({ background_color: value });
              }
            }
          },
          {
            get: () => config.gold_color || defaultConfig.gold_color,
            set: (value) => {
              config.gold_color = value;
              if (window.elementSdk) {
                window.elementSdk.setConfig({ gold_color: value });
              }
            }
          },
          {
            get: () => config.text_color || defaultConfig.text_color,
            set: (value) => {
              config.text_color = value;
              if (window.elementSdk) {
                window.elementSdk.setConfig({ text_color: value });
              }
            }
          },
          {
            get: () => config.button_bg_color || defaultConfig.button_bg_color,
            set: (value) => {
              config.button_bg_color = value;
              if (window.elementSdk) {
                window.elementSdk.setConfig({ button_bg_color: value });
              }
            }
          },
          {
            get: () => config.button_text_color || defaultConfig.button_text_color,
            set: (value) => {
              config.button_text_color = value;
              if (window.elementSdk) {
                window.elementSdk.setConfig({ button_text_color: value });
              }
            }
          }
        ],
        borderables: [],
        fontEditable: {
          get: () => config.font_family || defaultConfig.font_family,
          set: (value) => {
            config.font_family = value;
            if (window.elementSdk) {
              window.elementSdk.setConfig({ font_family: value });
            }
          }
        },
        fontSizeable: {
          get: () => config.font_size || defaultConfig.font_size,
          set: (value) => {
            config.font_size = value;
            if (window.elementSdk) {
              window.elementSdk.setConfig({ font_size: value });
            }
          }
        }
      };
    }

    function mapToEditPanelValues(config) {
      return new Map([
        ["hero_headline", config.hero_headline || defaultConfig.hero_headline],
        ["hero_subheadline", config.hero_subheadline || defaultConfig.hero_subheadline],
        ["hero_primary_btn", config.hero_primary_btn || defaultConfig.hero_primary_btn],
        ["hero_secondary_btn", config.hero_secondary_btn || defaultConfig.hero_secondary_btn],
        ["brand_title", config.brand_title || defaultConfig.brand_title],
        ["brand_text", config.brand_text || defaultConfig.brand_text],
        ["brand_btn", config.brand_btn || defaultConfig.brand_btn],
        ["collection_title", config.collection_title || defaultConfig.collection_title],
        ["collection_btn", config.collection_btn || defaultConfig.collection_btn],
        ["membership_title", config.membership_title || defaultConfig.membership_title],
        ["membership_text", config.membership_text || defaultConfig.membership_text],
        ["membership_btn", config.membership_btn || defaultConfig.membership_btn],
        ["concierge_title", config.concierge_title || defaultConfig.concierge_title],
        ["concierge_text", config.concierge_text || defaultConfig.concierge_text],
        ["concierge_btn", config.concierge_btn || defaultConfig.concierge_btn],
        ["journal_title", config.journal_title || defaultConfig.journal_title],
        ["journal_text", config.journal_text || defaultConfig.journal_text],
        ["journal_btn", config.journal_btn || defaultConfig.journal_btn]
      ]);
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
